{
  "name": "d-ts-doc",
  "version": "0.0.0",
  "description": "Comment a definition typescript and get Markdown !",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "watch": "nodemon src/index.js --exec babel-node",
    "dist": "babel src -d dist",
    "test": "node_modules/.bin/mocha test -R spec --recursive --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zacaria/d-ts-doc.git"
  },
  "author": "Zacaria",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zacaria/d-ts-doc/issues"
  },
  "homepage": "https://github.com/Zacaria/d-ts-doc#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-2": "^6.18.0",
    "browser-sync": "^2.18.2",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0"
  }
}
